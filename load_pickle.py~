#!/usr/bin/env python3
# -*- coding: utf-8 -*-

import pickle
import numpy as np
from course import Course

if __name__ == "__main__":
    classes = {}
    with open("course_map.txt", "rb") as logFile:
        classes = pickle.load(logFile)


    for key in classes:
        for class_a in classes[key]:
            class_a.display_course()
        exit()
