#!/usr/bin/env python3
# -*- coding: utf-8 -*-


class Schedule:
    def __init__(self):
        self.class_list = []

    def add(self, class_a):
        self.class_list.append(class_a)

    def display_schedule(self):
        print("-------------")
        print("SCHEDULE")
        for class_a in self.class_list:
            class_a.display_course()
        print("-------------")
